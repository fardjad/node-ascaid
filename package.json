{
  "name": "ascaid",
  "type": "module",
  "version": "1.8.15",
  "description": "Hassle-free documentation generation powered by AsciiDoc",
  "author": "Fardjad Davari <public@fardjad.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "lib/",
    "cli/",
    "ascaid.config.schema.json",
    "!lib/**/*.test.js",
    "!**/__fixtures__/"
  ],
  "keywords": [
    "asciidoc",
    "asciidoctor",
    "pandoc",
    "markdown",
    "confluence",
    "publish",
    "documentation"
  ],
  "scripts": {
    "prepare": "husky install",
    "fix": "eslint --fix . && prettier -w .",
    "test": "jest",
    "posttest": "eslint . && prettier -c ."
  },
  "bin": {
    "ascaid": "./cli/ascaid",
    "ascaid.js": "./cli/ascaid.js"
  },
  "main": "index.js",
  "dependencies": {
    "@asciidoctor/core": "^2.2.6",
    "ajv": "^8.12.0",
    "browser-sync": "^2.29.1",
    "commander": "^10.0.1",
    "confluence-api": "^1.4.0",
    "execa": "^7.1.1",
    "glob": "^10.2.2",
    "pify": "^6.1.0",
    "strip-json-comments": "^5.0.0"
  },
  "repository": "git@github.com:fardjad/node-ascaid.git",
  "devDependencies": {
    "asciidoctor-kroki": "^0.17.0",
    "eslint": "^8.39.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-light-runner": "^0.4.1",
    "npm-check-updates": "^16.10.9",
    "prettier": "^2.8.7"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
