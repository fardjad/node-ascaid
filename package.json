{
  "name": "ascaid",
  "type": "module",
  "version": "1.5.1",
  "description": "Hassle-free documentation generation powered by AsciiDoc",
  "author": "Fardjad Davari <public@fardjad.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "lib/",
    "cli/",
    "!lib/**/*.test.js",
    "!**/__fixtures__/"
  ],
  "keywords": [
    "asciidoc",
    "asciidoctor",
    "pandoc",
    "markdown",
    "confluence",
    "publish",
    "documentation",
    "kroki"
  ],
  "scripts": {
    "prepare": "husky install",
    "fix": "eslint --fix . && prettier -w .",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest",
    "posttest": "eslint . && prettier -c ."
  },
  "bin": {
    "ascaid": "./cli/ascaid",
    "ascaid.js": "./cli/ascaid.js"
  },
  "main": "index.js",
  "dependencies": {
    "@asciidoctor/core": "^2.2.6",
    "asciidoctor-kroki": "^0.15.4",
    "browser-sync": "^2.27.9",
    "commander": "^9.2.0",
    "confluence-api": "^1.4.0",
    "cross-env": "^7.0.3",
    "execa": "^6.1.0",
    "glob": "^8.0.1",
    "pify": "^5.0.0",
    "strip-json-comments": "^4.0.0"
  },
  "repository": "git@github.com:fardjad/node-ascaid.git",
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "jest": "^28.0.0",
    "prettier": "^2.6.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
